{
  "name": "deck-ng-angular",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "ionic": "ionic serve",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --configuration production",
    "build-android": "export CAPACITOR_ANDROID_STUDIO_PATH=/usr/local/bin/studio && ionic capacitor build android",
    "build-android-ci": "ionic capacitor copy android && cd android && ./gradlew bundleRelease && cd ..",
    "test": "ng test",
    "test-ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage --source-map true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate:api": "openapi-generator-cli generate -g typescript-angular -i ./deckAPI.yaml -o ./src/app --global-property models,apis,supportingFiles",
    "generate:ressources": "cordova-res android --skip-config --copy"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@angular/service-worker": "^14.2.0",
    "@capacitor/android": "4.1.0",
    "@capacitor/app": "4.0.1",
    "@capacitor/core": "4.1.0",
    "@capacitor/device": "4.0.1",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/ios": "4.1.0",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/splash-screen": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@ionic-native/core": "5.36.0",
    "@ionic-native/in-app-browser": "5.36.0",
    "@ionic/angular": "6.2.4",
    "@ionic/storage": "^3.0.6",
    "@ionic/storage-angular": "^3.0.6",
    "cordova-plugin-inappbrowser": "5.0.0",
    "core-js": "^3.25.0",
    "karma-coverage": "^2.2.0",
    "md5-typescript": "^1.0.5",
    "ngx-markdown": "^14.0.1",
    "rxjs": "^6.5.0",
    "sw-toolbox": "3.6.0",
    "swiper": "8.3.2",
    "tslib": "^2.4.0",
    "webdriver-manager": "^12.1.8",
    "zone.js": "~0.11.8"
  },
