{
  "name": "deck-ng",
  "version": "1.5.0",
  "author": "Mario Meltzow",
  "homepage": "https://meltzow.github.io/deck-ng/",
  "scripts": {
    "ng": "ng",
    "ionic": "ionic serve -- --proxy-config proxy.conf.json",
    "run-android": "export ANDROID_SDK_ROOT=$HOME/Programme/android-sdk-linux/ && ionic capacitor run android -l --external",
    "run-ios": "ionic capacitor run ios -l --external",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --configuration production --prod",
    "build-android": "export CAPACITOR_ANDROID_STUDIO_PATH=/usr/local/bin/studio && ionic capacitor sync android && ionic capacitor open android",
    "build-android-ci": "ionic capacitor sync android --prod && cd android && ./gradlew bundleRelease && cd ..",
    "build-ios-ci": "ionic capacitor sync ios --prod",
    "test": "ng test",
    "test-ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage --source-map true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate:ressources": "npm run capacitor-assets generate",
    "capacitor-assets": "capacitor-assets"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.1",
    "@angular/common": "^15.1.1",
    "@angular/core": "^15.1.1",
    "@angular/forms": "^15.1.1",
    "@angular/material": "^15.1.1",
    "@angular/platform-browser": "^15.1.1",
    "@angular/platform-browser-dynamic": "^15.1.1",
    "@angular/router": "^15.1.1",
    "@angular/service-worker": "^15.1.1",
    "@capacitor-community/barcode-scanner": "^3.0.3",
    "@capacitor/android": "4.7.1",
    "@capacitor/app": "4.1.1",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/browser": "^4.1.1",
    "@capacitor/camera": "^4.1.4",
    "@capacitor/core": "4.7.1",
    "@capacitor/device": "4.1.0",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.7.1",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/splash-screen": "^4.2.0",
    "@capacitor/status-bar": "^4.1.1",
    "@ionic/angular": "7.0.6",
    "@ionic/pwa-elements": "^3.1.1",
    "@ionic/storage": "^3.0.6",
    "@ionic/storage-angular": "^3.0.6",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/markdown-it": "^12.2.3",
    "core-js": "^3.29.1",
    "karma-coverage": "^2.2.0",
    "markdown-it": "^13.0.1",
    "markdown-it-task-checkbox": "^1.0.6",
    "md5-typescript": "^1.0.5",
    "q": "^1.5.1",
    "rxjs": "^7.8.0",
    "sw-toolbox": "3.6.0",
    "tslib": "^2.5.0",
    "webdriver-manager": "^12.1.9"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1501.2",
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular-devkit/core": "^15.1.2",
    "@angular-devkit/schematics": "^15.1.2",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "^15.1.2",
    "@angular/compiler": "^15.1.1",
    "@angular/compiler-cli": "^15.1.1",
    "@angular/language-service": "^15.1.1",
    "@capacitor/cli": "4.7.3",
    "@ionic/angular-toolkit": "9.0.0",
    "@ionic/cli": "7.1.1",
    "@ionic/lab": "^3.2.15",
    "@types/jasmine": "~4.3.1",
    "@types/jasmine-ajax": "^3.3.3",
    "@types/jasminewd2": "2.0.10",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "8.37.0",
    "eslint-plugin-import": "^2.27.5",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ngx-translate-testing": "^6.1.0",
    "protractor": "~7.0.0",
    "puppeteer": "^19.8.2",
    "ts-node": "10.9.1",
    "tslint": "~6.1.3",
    "typescript": "~4.9.4",
    "whatwg-fetch": "^3.6.2"
  }
}
